{
  "version": 2,
  "name": "cti-ioc-lookup",
  "builds": [
    {
      "src": "backend/main.py",
      "use": "@vercel/python"
    },
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/main.py"
    },
    {
      "src": "/health",
      "dest": "backend/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "frontend/build/$1"
    }
  ],
  "env": {
    "VIRUSTOTAL_API_KEY": "",
    "ABUSEIPDB_API_KEY": "",
    "URLSCAN_API_KEY": "",
    "OTX_API_KEY": "",
    "GREYNOISE_API_KEY": "",
    "LOG_LEVEL": "INFO",
    "CORS_ORIGINS": "*"
  },
  "build": {
    "env": {
      "REACT_APP_BACKEND_URL": ""
    }
  }
}
